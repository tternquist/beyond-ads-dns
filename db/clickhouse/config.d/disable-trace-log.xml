<!-- Disable trace_log and reduce logger verbosity to cut I/O and log volume. -->
<!-- trace_log: query profiling (system.trace_log) -->
<!-- logger level: main log verbosity; "information" suppresses trace/debug -->
<!-- See https://clickhouse.com/docs/en/operations/system-tables/trace_log -->
<clickhouse>
    <trace_log remove="1"/>
    <logger>
        <level>information</level>
    </logger>
</clickhouse>
